<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
   	<package name="ftpUser" extends="struts-default"  namespace="/ftpUser">
          <action name="ftpUserIndex" class="com.ailk.eaap.op2.conf.catalog.FtpUserManagerAction" method="ftpUserIndex">
          		<result name="success">/ftpWeb/ftpUserIndex.jsp</result>
          </action>
          <action name="showftpUserGrid" class="com.ailk.eaap.op2.conf.catalog.FtpUserManagerAction" method="showftpUserGrid">
          		<result name="success">/ftpWeb/ftpUserIndex.jsp</result>
          </action>
          <action name="ftpUserAdd" class="com.ailk.eaap.op2.conf.catalog.FtpUserManagerAction" method="ftpUserAdd">
          		<result name="success">/ftpWeb/ftpUserAdd.jsp</result>
          </action>
          <action name="ftpUserAddData" class="com.ailk.eaap.op2.conf.catalog.FtpUserManagerAction" method="ftpUserAddData">
          		<result name="success">/ftpWeb/ftpUserIndex.jsp</result>
          </action>
          <action name="ftpUserUpdate" class="com.ailk.eaap.op2.conf.catalog.FtpUserManagerAction" method="ftpUserUpdate">
          		<result name="success">/ftpWeb/ftpUserUpdate.jsp</result>
          </action>
          <action name="ftpUserUpdateData" class="com.ailk.eaap.op2.conf.catalog.FtpUserManagerAction" method="ftpUserUpdateData">
          		<result name="success">/ftpWeb/ftpUserIndex.jsp</result>
          </action>
    </package>
    </struts>